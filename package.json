  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "^0.1.45",
    "@swc/core": "^1.2.59",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.3.2"
  },
  "files": [
    "lib/main.js",
    "lib/main.old.js"
  ],
  "scripts": {
    "build:esm": "swc ./src/main.ts -o lib/main.old.js --config module.type=es6",
